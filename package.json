{
  "name": "truststack-link-resolver",
  "version": "0.0.1",
  "private": true,
  "description": "",
  "license": "UNLICENSED",
  "author": "",
  "main": "dist/src/extension.js",
  "scripts": {
    "build": "npx prisma generate && nest build",
    "docker:down": "docker compose down",
    "docker:up": "docker compose up -d postgres",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "gen:openapi": "ts-node -r tsconfig-paths/register ./scripts/gen-openapi.ts",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "precommit": "pnpm gen:openapi",
    "prepare": "husky",
    "prisma:generate": "npx prisma generate",
    "prisma:push": "npx prisma db push",
    "start": "nest start",
    "start:debug": "nest start --debug --watch",
    "start:dev": "nest start --watch",
    "start:dev:push": "pnpm run prisma:push && nest start --watch",
    "start:prod": "npx prisma migrate deploy && node dist/src/main",
    "test": "vitest",
    "test:cov": "vitest --coverage",
    "test:e2e": "vitest --config vitest.e2e.config.ts",
    "test:watch": "vitest --watch"
  },
  "dependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^10.0.0",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/swagger": "^8.1.0",
    "@prisma/client": "^5.22.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "dataloader": "^2.2.3",
    "dotenv": "^16.4.5",
    "express": "4.21.2",
    "lodash": "^4.17.21",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.4.1",
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/express": "^4.17.17",
    "@types/lodash": "^4.17.6",
    "@types/node": "^20.3.1",
    "@types/pg": "^8.11.13",
    "@types/supertest": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.7",
    "pg": "^8.14.1",
    "prettier": "^3.3.2",
    "prisma": "^5.22.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.4",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3",
    "unplugin-swc": "^1.5.1",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^2.1.1"
  }
}